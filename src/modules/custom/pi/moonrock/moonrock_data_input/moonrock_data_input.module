<?php 



function moonrock_data_input_form_pi_data_node_form_alter(&$form, &$form_state, $form_id = NULL) {
	global $user;
	$node_details = get_inquiry_details_from_current_path();
	$sample_view_nid = find_unique_node_type_for_uid_and_inquiry('moonrock_seesamples', $user->uid, $node_details->inquiry_id);
	if ($sample_view_nid) {
		$form['#prefix'] = _moonrock_data_input_sampletable($sample_view_nid);
	}
	return;
}

function _moonrock_data_input_sampletable($nid) {
	$sample_view = node_load($nid);
	module_load_include('inc', 'moonrock_seesamples', 'moonrock_seesamples.table');
	drupal_add_js( drupal_get_path('module', 'moonrock_data_input') .'/js/sampleSelectionHelper.js');
	
	return moonrock_seesamples_create_sample_table($sample_view);
}